{
  "description" : "Image collection format for Sentinel 2 Level 1C data as downloaded from the Google Cloud Bucket, expects a list of file paths as input..",
  "tags" : ["Sentinel", "Google Bucket", "ESA", "TOA"],
  "pattern" : ".+/IMG_DATA/.+(\\.jp2|\\.tif)",
  "images" : {
    "pattern" : ".*/(.+)\\.SAFE.*"
  },
  "datetime" : {
    "pattern" : ".*MSIL1C_(.+?)_.*",
    "format" : "%Y%m%dT%H%M%S"
  },
  "bands" : {
    "band_1" : {
      "nodata" : 0,
      "scale" : 0.0001,
      "pattern" : ".+_B01\\.jp2"
    },
    "band_2" : {
      "nodata" : 0,
      "scale" : 0.0001,
      "pattern" : ".+_B02\\.jp2"
    },
    "band_3" : {
      "nodata" : 0,
      "scale" : 0.0001,
      "pattern" : ".+_B03\\.jp2"
    },
    "band_4" : {
      "nodata" : 0,
      "scale" : 0.0001,
      "pattern" : ".+_B04\\.jp2"
    },
    "band_5" : {
      "nodata" : 0,
      "scale" : 0.0001,
      "pattern" : ".+_B05\\.jp2"
    },
    "band_6" : {
      "nodata" : 0,
      "scale" : 0.0001,
      "pattern" : ".+_B06\\.jp2"
    },
    "band_7" : {
      "nodata" : 0,
      "scale" : 0.0001,
      "pattern" : ".+_B07\\.jp2"
    },
    "band_8" : {
      "nodata" : 0,
      "scale" : 0.0001,
      "pattern" : ".+_B08\\.jp2"
    },
    "band_8A" : {
      "nodata" : 0,
      "scale" : 0.0001,
      "pattern" : ".+_B8A\\.jp2"
    },
    "band_9" : {
      "nodata" : 0,
      "scale" : 0.0001,
      "pattern" : ".+_B09\\.jp2"
    },
    "band_10" : {
      "nodata" : 0,
      "scale" : 0.0001,
      "pattern" : ".+_B10\\.jp2"
    },
    "band_11" : {
      "nodata" : 0,
      "scale" : 0.0001,
      "pattern" : ".+_B11\\.jp2"
    },
    "band_12" : {
      "nodata" : 0,
      "scale" : 0.0001,
      "pattern" : ".+_B12\\.jp2"
    },
    "CM" : {
      "nodata" : 255,
      "scale" : 1,
      "pattern" : ".+_CM\\.tif"
    }
  }
}